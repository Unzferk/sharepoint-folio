{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "linechart-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/linechart/LinechartWebPart.js",
          "manifest": "./src/webparts/linechart/LinechartWebPart.manifest.json"
        }
      ]
    },
    "piechart-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/piechart/PiechartWebPart.js",
          "manifest": "./src/webparts/piechart/PiechartWebPart.manifest.json"
        }
      ]
    },
    "customers-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/customers/CustomersWebPart.js",
          "manifest": "./src/webparts/customers/CustomersWebPart.manifest.json"
        }
      ]
    },
    "owners-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/owners/OwnersWebPart.js",
          "manifest": "./src/webparts/owners/OwnersWebPart.manifest.json"
        }
      ]
    },
    "graphics-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/graphics/GraphicsWebPart.js",
          "manifest": "./src/webparts/graphics/GraphicsWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "LinechartWebPartStrings": "lib/webparts/linechart/loc/{locale}.js",
    "PiechartWebPartStrings": "lib/webparts/piechart/loc/{locale}.js",
    "CustomersWebPartStrings": "lib/webparts/customers/loc/{locale}.js",
    "OwnersWebPartStrings": "lib/webparts/owners/loc/{locale}.js",
    "GraphicsWebPartStrings": "lib/webparts/graphics/loc/{locale}.js"
  }
}
